<?xml version="1.0" encoding="utf-8"?>
<!-- This is a simple XML definition for a Drawable of subclass ShapeDrawable -> A goldren circle moon rectangle. -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
        android:shape="oval"
      android:useLevel="false" >
    <solid android:color="@color/moon"/>
    <gradient
        android:angle="90"
        android:endColor="@color/moon2"
        android:startColor="@color/moon" />
    <size android:width="120dp"    android:height="120dp"/>
</shape>
